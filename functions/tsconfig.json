{
  "compilerOptions": {
    "target": "ES2020",
    "module": "node16",
    "lib": ["ES2020"],
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "@utils/*": ["utils/*"],
      "@services/*": ["services/*"],
      "@env": ["env"]
    },
    "outDir": "lib"
  },
  "include": ["src"],
  "extends": "expo/tsconfig.base"
}
